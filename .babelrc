{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-es2015-modules-umd",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ],
  "ignore": "/lib|node_modules|.spec.js/",
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}