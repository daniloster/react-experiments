{
  "name": "daniloster-cli",
  "version": "0.0.5",
  "description": "CLI to the lerna repo for react experiments",
  "main": "index.js",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [],
    "exclude": [
      "src/**/*",
      "test",
      "build",
      "dist",
      "lib",
      "mochawesome-reports"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check-coverage": "exit 0",
    "coverage": "exit 0",
    "test": "npm run lint",
    "precompile": "rm -rf lib/ && mkdir -p lib",
    "compile": "babel src --out-dir lib --copy-files --source-maps",
    "prepublish": "npm run compile",
    "lint:fix": "eslint --fix src --config ../../.eslintrc.json --ignore-path ../../.eslintignore",
    "lint": "eslint src --config ../../.eslintrc.json --ignore-path ../../.eslintignore"
  },
  "bin": {
    "daniloster-cli": "./index.js"
  },
  "dependencies": {
    "axios": "0.16.2",
    "camelize": "1.0.0",
    "co": "4.6.0",
    "co-prompt": "1.0.0",
    "commander": "2.11.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "cross-env": "3.1.4",
    "enzyme": "2.7.0",
    "eslint": "3.19.0",
    "mocha": "3.2.0",
    "mochawesome": "2.0.2",
    "nyc": "10.0.0",
    "react-addons-test-utils": "15.4.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  }
}
