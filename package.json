{
  "name": "react-experiments",
  "version": "0.0.1",
  "engines": {
    "node": ">6.0.0"
  },
  "dependencies": {
    "axios": "0.16.2",
    "mobx": "3.3.0",
    "mobx-react": "4.3.2",
    "moment": "2.18.1",
    "moment-range": "3.0.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.1",
    "babel-istanbul": "0.12.2",
    "babel-loader": "7.1.2",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-regenerator-runtime": "6.5.0",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "chai": "4.1.2",
    "cross-env": "5.0.5",
    "css-loader": "0.25.0",
    "daniloster-cli": ">=0.0.1",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "enzyme-adapter-utils": "1.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.26.0",
    "ignore-styles": "5.0.1",
    "img-loader": "1.3.1",
    "istanbul": "0.4.5",
    "jsdom": "11.3.0",
    "json-loader": "0.5.4",
    "lerna": "2.8.0",
    "mocha": "3.5.3",
    "mochawesome": "2.3.1",
    "node-sass": "4.5.3",
    "nyc": "10.0.0",
    "prettier": "1.10.2",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.6.2",
    "react-asciidoc-generator": "1.5.8",
    "react-test-renderer": "16.0.0",
    "sass-loader": "4.1.1",
    "sinon": "4.0.0",
    "source-map-loader": "0.1.5",
    "source-map-support": "0.4.8",
    "style-loader": "0.13.1",
    "svg-url-loader": "2.2.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2",
    "webpack-node-externals": "1.5.4",
    "yarn": "1.3.2"
  },
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "cli": "daniloster-cli",
    "install:yarn":
      "npm install yarn@$(cat package.json | grep \"yarn\" | awk '{print $2}' | cut -d'\"' -f2) -g",
    "install:lerna":
      "npm install lerna@$(cat lerna.json | grep \"lerna\" | awk '{print $2}' | cut -d'\"' -f2) -g",
    "tools": "npm run install:yarn && yarn config set workspaces-experimental true",
    "bootstrap": "lerna bootstrap --concurrency 8",
    "prepare": "lerna run compile --parallel --concurrency 8",
    "test": "lerna run test --since --concurrency 8",
    "coverage": "lerna run coverage --since --concurrency 8",
    "check-coverage": "lerna run check-coverage --since --concurrency 8",
    "validate": "npm run coverage && npm run check-coverage",
    "doc": "lerna run doc --parallel --concurrency 8"
  }
}
